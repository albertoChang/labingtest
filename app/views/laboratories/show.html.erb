<div class="card shadow mb-4">
  <div class="card-header py-3 ">
    <h1 class="m-0 font-weight-bold text-primary">Laboratorio <small>Nuevo</small></h1>
  </div>
  <div class="form-group row">
    <label class= "col-md-3 col-form-label">Nombre</label>
    <div class="col-md-9">
      <%= @laboratory.name %>
    </div>
  </div>
  <div class="form-group row">
    <label class= "col-md-3 col-form-label">Correo Electrónico</label>
    <div class="col-md-9">
      <%= @laboratory.email %>
    </div>
  </div>
  <div class="form-group row">
    <label class= "col-md-3 col-form-label">Dirección</label>
    <div class="col-md-9">
      <%= @laboratory.address %>
    </div>
  </div>
  <div class="form-group row">
    <label class= "col-md-3 col-form-label">Teléfono</label>
    <div class="col-md-9">
      <%= @laboratory.phone %>
    </div>
  </div>
  <div class="form-group row">
    <label class= "col-md-3 col-form-label">Descripción</label>
    <div class="col-md-9">
      <%= @laboratory.description %>
    </div>
  </div>
  <div class="sical-form-buttons">
    <div class="text-center">
      <%= link_to "Regresar", laboratories_path, class: "btn btn-primary" %>
    </div>
  </div>
</div>
